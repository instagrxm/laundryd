{
  "name": "laundry",
  "description": "data laundering tools",
  "version": "0.0.0",
  "author": "Josh Santangelo @endquote",
  "bin": {
    "laundry": "./bin/run"
  },
  "bugs": "https://github.com/endquote/laundryd/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-commands": "^1.2.3",
    "@oclif/plugin-help": "^2.2.3",
    "arr-unique": "^1.0.2",
    "array-unique": "^0.3.2",
    "cron": "^1.8.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/cron": "^1.7.1",
    "@types/node": "^10.17.15",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/endquote/laundryd",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "repositoryPrefix": "<%- repo %>/blob/master/<%- commandPath %>",
    "bin": "laundry",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands"
    ]
  },
  "repository": "endquote/laundryd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && cd docs && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "cd docs && oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
