    "commands": "./lib/commands",
    "repositoryPrefix": "<%- repo %>/blob/master/<%- commandPath %>",
    "bin": "laundry",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands"
    ]
  },
  "repository": "laundryd/laundry",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
