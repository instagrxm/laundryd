{
  "name": "laundry",
  "description": "data laundering tools",
  "version": "0.0.0",
  "author": "Josh Santangelo @endquote",
  "bin": {
    "laundry": "./bin/run"
  },
  "bugs": "https://github.com/endquote/laundryd/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-commands": "^1.2.3",
    "@oclif/plugin-help": "^2.2.3",
    "arr-unique": "^1.0.2",
    "array-unique": "^0.3.2",
    "aws-sdk": "^2.623.0",
    "axios": "^0.19.2",
    "cron": "^1.8.2",
    "ffbinaries": "^1.1.4",
    "filenamify-url": "^2.1.1",
    "fs-extra": "^8.1.0",
    "is-url": "^1.2.4",
    "mime": "^2.4.4",
    "mongodb": "^3.5.3",
    "shortid": "^2.2.15",
    "tslib": "^1.10.0",
    "youtube-dl.js": "^1.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/cron": "^1.7.1",
    "@types/fs-extra": "^8.1.0",
    "@types/is-url": "^1.2.28",
    "@types/mime": "^2.0.1",
    "@types/mongodb": "^3.3.16",
    "@types/node": "^10.17.15",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^5.16.0",
    "globby": "^10.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/endquote/laundryd",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "repositoryPrefix": "<%- repo %>/blob/master/<%- commandPath %>",
    "bin": "laundry",
    "hooks": {
      "prerun": [
        "./lib/hooks/prerun/prerun"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-commands"
    ]
  },
  "repository": "endquote/laundryd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && cd docs && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "cd docs && oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
